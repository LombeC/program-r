<?xml version="1.0" encoding="UTF-8"?>

<aiml version = "2.0">
   <category>
      <pattern>test </pattern>

      <template>
           ARE YOU ORIGINALLY FROM COLORADO?

           <oob>
                <robot>
                    <options>
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                    </options>

                <image>
                    <filename>pic.jpg</filename>
                    <duration>10</duration>
                </image>

                <video>
                    <filename>f.mp4</filename>
                </video>

                </robot>
           </oob>

      </template>
   </category>



   <category>
        <pattern># <set>NO</set> ^</pattern>
        <that>ARE YOU ORIGINALLY FROM COLORADO?</that>
        <template>
            Oh. Then where from?
            <oob>
                <robot>
                    <options>
                        <option>Yes</option>
                        <option>No</option>
                    </options>
                </robot>
            </oob>
        </template>
    </category>


</aiml>
