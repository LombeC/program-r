<aiml version="2.0">



    <!-- STARTING THE SESSION -->


    <category>
        <pattern>START SESSION 1</pattern>
        <template>Ok. Let's begin Session 1. How are you doing today?</template>
    </category>


    <!--

    <category>
        <pattern><set>greetings</set></pattern>
        <template>
            <random>
                <li>Hello!</li>
                <li>Hey! It is nice to meet you!</li>
                <li>Howdy! It is a pleasure to meet you!</li>
                <li>Hi There!</li>
                <li>Hi!</li>
                <li>Hey!</li>
                <li>Hello, it is nice to meet you.</li>
