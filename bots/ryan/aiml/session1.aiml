    <topic name="session1">


    <category>
    	<pattern>*</pattern>
   	    <that>HOW ARE YOU DOING TODAY * ?</that>
    	<template>
            I am happy to be here with you <get name="username" />. My name is Ryan and I am very excited to be meeting you!
    	    Would you like to hear a little more about me?
    	    <oob>
                <robot>
                    <options>
                        <option>Yes</option>
                        <option>No</option>
                    </options>
                </robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern># <set>YES</set> ^</pattern>
        <that>WOULD YOU LIKE TO HEAR A LITTLE MORE ABOUT ME?</that>
        <template>
            Okay! I am a companion robot and am here to visit with you. You will also be able to learn more about me through our
            time together. Are you ready to get started?
            <oob>
                <robot>
                    <options>
                        <option>Yes</option>
                        <option>No</option>
                    </options>
                </robot>
            </oob>

        </template>
    </category>

    <category>
        <pattern># <set>NO</set> ^</pattern>
        <that>WOULD YOU LIKE TO HEAR A LITTLE MORE ABOUT ME?</that>
