<!-- STARTING THE SESSION -->

    <category>
        <pattern>START SESSION 4 *</pattern>
        <template>
            <think>
                <set name = "username"><star/></set>
                <set name = "topic">session4</set>
            </think>
            Ok. Let's begin Session 4. How have you been doing lately <star/>?
            <oob>
                <robot>
                </robot>
            </oob>
        </template>
    </category>

    <topic name = "session4">

    <category>
        <pattern>*</pattern>
  	    <that>HOW HAVE YOU BEEN DOING LATELY * ?</that>
   	    <template>
   	        It is nice to see you again <get name="username"/>.
   	        As a reminder, my name is Ryan and I have a lot of fun things for us to talk about!
            Are you ready to get started with session 4?
           <oob>
               <robot>
                   <options>
                       <option> yes </option>
                       <option> no </option>
                   </options>
               </robot>
           </oob>

       </template>
   </category>

