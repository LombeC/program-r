    <!-- CATEGORIES TO SWITCH TOPICS AND GET OUT OF CURRENT DISCUSSION -->
    <category>
        <pattern>$Change topic</pattern>
        <template>
            Ok. What would you like to talk about? Some of my favorite things to talk about are <bot name="talkabout"/>
            <oob><robot></robot></oob>
        </template>
    </category>
    <category>
        <pattern>CHANGE TOPIC</pattern>
        <template><srai>Change topic</srai></template>
    </category>

    <category>
