<aiml version="2.0">
    <!-- BRANCHING FROM SCIENCE2 ASKING ABOUT FAVORITE PLANET -->
    <category>
        <pattern># PLUTO ^</pattern>
        <that>Do you have a favorite planet?</that>
        <template>
            Did you know that scientists don't consider Pluto a planet any longer?
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

    <category>
