<!-- STARTING THE SESSION -->

   <category>
       <pattern>START SESSION 3 *</pattern>
       <template>
            <think>
                <set name = "username"><star/></set>
                <set name="topic">session3</set>
            </think>
            Welcome back <get name="username"/>! I am happy to be chatting with you again. How are you?
           <oob>
               <robot>
               </robot>
           </oob>
       </template>
   </category>

   <topic name="session3">

    <category>
        <pattern>*</pattern>
        <that>HOW ARE YOU ON THIS FINE DAY * ?</that>
        <template>
            <think>
                <set name="data"><star/></set>
            </think>
            <condition name="getsentiment">
                <li value="verypositive">
                    That is so good to hear! As a reminder, I am Ryan and I have a lot of fun things for us to talk about!
                    Are you ready to get started with today's conversation?
                    <oob>
