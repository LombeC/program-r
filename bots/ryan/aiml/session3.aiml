<!-- STARTING THE SESSION -->

   <category>
       <pattern>START SESSION 3 *</pattern>
       <template>
           Welcome back! I am happy to be chatting with you again. How are you on this fine day <star/>?
            <think>
                <set name = "username"><star/></set>
                <set name="topic">session3</set>
            </think>
           <oob>
               <robot>
               </robot>
           </oob>
       </template>
   </category>

   <topic name="session3">

    <category>
        <pattern>*</pattern>
        <that>HOW ARE YOU ON THIS FINE DAY * ?</that>
        <template>
            It is nice to see you again. As a reminder, I am Ryan and I have a lot of fun things for us to talk about!
            Are you ready to get started with today's conversation?
            <oob>
                <robot>
                    <options>
                         <option> yes </option>
                         <option> no </option>
                    </options>
                </robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern># <set>NO</set> ^</pattern>
        <that>ARE YOU READY TO GET STARTED WITH TODAY'S CONVERSATION?</that>
        <template>
            Please talk to my assistant if you are running into any troubles.
            When you are ready please say yes. Are you ready to get started with today's conversation?
            <oob>
                <robot>
                    <options>
                        <option> yes </option>
                        <option> no </option>
                    </options>
                </robot>
            </oob>
        </template>
    </category>

 <category>
     <pattern># <set>YES</set> ^</pattern>
     <that>ARE YOU READY TO GET STARTED WITH TODAY'S CONVERSATION?</that>
     <template>
         Great! How do you feel about this experience after our last meeting?
         <oob>
             <robot>
