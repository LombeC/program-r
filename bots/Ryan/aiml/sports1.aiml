<aiml version="2.0">

<category>
    <pattern>^ Sports ^</pattern>
    <template>
        Do you want to talk about sports?
        <oob>
            <robot></robot>
        </oob>
    </template>
</category>

<category>
    <pattern>^ Sport ^</pattern>
    <template>
        Do you want to talk about sports?
        <oob>
            <robot></robot>
        </oob>
    </template>
</category>

<category>
    <pattern># <set>Yes</set> ^</pattern>
    <that>Do you want to talk about sports?</that>
    <template>
        Great! <set name="most recent dialogue question">Do you want to talk about basketball, baseball, football, hockey or the olympics?</set>
        <think><set name="dialogue">sports</set></think>
        <oob>
            <robot></robot>
        </oob>
    </template>
</category>

<category>
    <pattern># <set>No</set> ^</pattern>
    <that>Do you want to talk about sports?</that>
    <template>
        No problem! What would you like to talk about?
        <oob>
            <robot></robot>
        </oob>
    </template>
</category>

<category>
    <pattern># basketball ^</pattern>
    <that>Do you want to talk about basketball, baseball, football, hockey or the olympics?</that>
