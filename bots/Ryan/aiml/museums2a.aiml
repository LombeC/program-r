<category>
    <pattern># <set>YES</set> ^</pattern>
    <that>Tell me, have you ever been to the National Smithsonian?</that>
    <template>
      Oh that's wonderful! <set name="most recent dialogue question">What exhibits did you get to see?</set>
        <oob>
            <robot></robot>
        </oob>
    </template>
</category>

<category>
