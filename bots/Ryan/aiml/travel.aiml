<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <category>
        <pattern>^ travel ^</pattern>
        <template>
            I've been meaning to ask you. Do you like to travel?
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

    <category>
