<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <category>
        <pattern># <set>YES</set> ^</pattern>
        <that>How about a different cuisine?</that>
        <template>
            Would you like to talk about a American Food or foreign food?
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern># FOREIGN ^</pattern>
        <that>WOULD YOU LIKE TO TALK ABOUT A AMERICAN FOOD OR FOREIGN FOOD?</that>
        <template>
            <random>
                <li>Do you like Asian food?</li>
                <li>Do you like Mexican food?</li></random>
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern># <set>YES</set> ^</pattern>
        <that>Do you like Mexican food?</that>
        <template>
            <think><set name="topic">mexicanfood</set></think>

            You know that sound that fajita's make when they come out of the kitchen sizzling?
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

