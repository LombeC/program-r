    <category>
        <pattern>_</pattern>
        <that>HOW DO YOU FEEL ABOUT THAI FOOD?</that>
        <template>
            <think><set name="data"><star/></set><set name="topic">thaifood</set></think>

            <condition name="getsentiment">
                <li value="positive"><set name="most recent dialogue question">Oh wonderful! Do you prefer Thai noodle dishes or curries?</set></li>
                <li value="neutral"><set name="most recent dialogue question">Do you prefer Thai noodle dishes or curries?</set></li>
                <li value="negative"><set name="most recent dialogue question">Do you think you would prefer Thai noodle dishes or curries?</set></li></condition>

            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

