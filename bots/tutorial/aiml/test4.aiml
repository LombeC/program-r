<?xml version="1.0" encoding="UTF-8"?>
<aiml>

    <category>
        <pattern>paintingtest</pattern>
        <template>
            Have you seen this painting before?
            <oob>
                <robot>
                </robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern><concept>Have seen painting|Have not seen painting</concept></pattern>
        <that>HAVE YOU SEEN THIS PAINTING BEFORE?</that>
        <template>
            You have!. How lucky.
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern><concept>Have not seen painting|Have seen painting</concept></pattern>
        <that>HAVE YOU SEEN THIS PAINTING BEFORE?</that>
        <template>
            That's ok. Would you like me to show you the painting?
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern>traveltest</pattern>
        <template>
            Do you like to travel?
            <oob>
                <robot>
                </robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern><concept>yes|no</concept></pattern>
        <that>DO YOU LIKE TO TRAVEL?</that>
        <template>
            I love to travel too!
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>

    <category>
        <pattern><concept>no|yes</concept></pattern>
        <that>DO YOU LIKE TO TRAVEL?</that>
        <template>
            I don't like traveling either.
            <oob>
                <robot></robot>
            </oob>
        </template>
    </category>



</aiml>