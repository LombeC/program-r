<?xml version="1.0" encoding="UTF-8"?>
<aiml>
    <category>
        <pattern>HELLO ^</pattern>
        <template>
                im good? How are you doing?(good|bad)
        </template>
    </category>


    <category>
        <pattern>GOOD</pattern>
        <that>How are you doing?(good|bad)</that>
        <template>
            excellent#http://www.google.com
        </template>
    </category>


    <category>
